{% extends "layout.html.twig" %}
{% set active="users"  %}
{% set version="Interface de jeu version 1.0"  %}
{% block content %}
        <div class="row">
            <div class="col-xs-12 col-sm-12">
                  <div class="list-group">
                      <a href="#" class="list-group-item active">
                          Liste des joueurs
                      </a>
                      {% for user in users %}
                        <a href="{{ path('user', {'id': user.id}) }}" class="list-group-item">
                            {{ user.username }}
                        </a>
                      {% endfor %}
                  </div>
            </div>
        </div>
{% endblock %}